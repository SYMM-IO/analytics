[Unit]
Description=Running the FastApi server
After=postgresql.service
Requires=postgresql.service
PartOf=analytics.target

[Service]
User=analytics_user
ExecStart=runner ANALYTICS_PATH/src/app/server.py
RestartSec=1
Restart=always
TimeoutStartSec=60
TimeoutStopSec=60

[Install]
WantedBy=analytics.target
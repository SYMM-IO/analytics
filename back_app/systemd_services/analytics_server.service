[Unit]
Description=Running the FastApi server
After=postgresql.service
Requires=postgresql.service
PartOf=analytics.target

[Service]
User=analytics_user
ExecStart=PYTHONPATH=ANALYTICS_PATH TENANT=BNB_8_2 python ANALYTICS_PATH/app/server.py
RestartSec=1
Restart=always
TimeoutStartSec=60
TimeoutStopSec=60

[Install]
WantedBy=analytics.target